{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Remix",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": [
      "@remix-run/node",
      "vite/client",
      "vitest/globals",
      "react",
      "@vitest/browser/providers/playwright"
    ],
    "jsx": "react-jsx",
    "noEmit": true
  }
}
